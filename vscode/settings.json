{
  "recommendations": [
    "vscode-icons-team.vscode-icons",
    // mac 本体にもneovimをインストールしておくこと
    "vscode-neovim.neovim",
    "enhancedjax.vscode-ayu-zed",
    "BrandonKirbyson.vscode-animations",
    "ryuta46.multi-command",
    "TomRijndorp.find-it-faster",
    "Gxl.git-graph-3",
    "mechatroner.rainbow-csv",
    "streetsidesoftware.code-spell-checker",
    "alefragnani.project-manager",
    // "iuyoy.highlight-string-code",
    "dprint.dprint"
  ],
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.linux": "$HOME/.config/nvim/init.lua",
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "$HOME/.config/nvim/init.lua",
  "vscode-neovim.useCtrlKeys": true,
  "vscode-neovim.compositor": false,
  // "workbench.list.automaticKeyboardNavigation": false,
  "terminal.integrated.scrollback": 30000,
  "debug.allowBreakpointsEverywhere": true,
  "security.workspace.trust.untrustedFiles": "open",
  // 行の折返し
  "editor.wordWrap": "on",
  "hadolint.hadolintPath": "/opt/homebrew/bin/hadolint",
  // ====================================
  // ファイル保存時に行われるアクション
  // ====================================
  // ファイル保存時、コードを整形する
  "editor.formatOnSave": true,
  // // ファイル保存時、同時に行われるアクション
  // "editor.codeActionsOnSave": {
  //   // 拡張機能Prettierのチェックを走らせる
  //   "source.fixAll.prettier": true
  // },
  // ファイル保存時、最終行に改行を挿入する
  "files.insertFinalNewline": true,
  // ファイル保存時、最終行以降の空行を削除する
  "files.trimFinalNewlines": true,
  // ファイル保存時、末尾の空白をトリミングする
  "files.trimTrailingWhitespace": true,
  // ミニマップを表示する
  "editor.minimap.enabled": false,
  // ファイルのパンくずリストを表示する
  "breadcrumbs.enabled": false,
  // メニューバーを表示する (toggle: Altキーで上部に表示する)
  "window.menuBarVisibility": "toggle",
  // ====================================
  // エディタのUIに関する詳細設定
  // ====================================
  // タブサイズ
  "editor.tabSize": 2,
  // インデントガイドを表示する
  "editor.guides.indentation": false,
  // 括弧（ブラケット）のペアガイドを表示する
  "editor.guides.bracketPairs": true,
  // 括弧（ブラケット）の水平方向のペアガイドを表示する
  "editor.guides.bracketPairsHorizontal": true,
  // 括弧（ブラケット）のペアごとに色を分ける
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  // 選択範囲のハイライトに角丸を適用する
  "editor.roundedSelection": false,
  // 左端の行番号にカーソル位置からの距離を表示
  // "editor.lineNumbers": "relative",
  // ====================================
  // ワークベンチの設定
  // ====================================
  // アイコンテーマ
  // "workbench.iconTheme": "material-icon-theme",
  "workbench.iconTheme": "ayu-mirage-zed",
  // ツリーのインデント
  "workbench.tree.indent": 12,
  // ツリーのインデントガイドを表示する
  "workbench.tree.renderIndentGuides": "always",
  // アニメーションを減らす(VSCode Animationsを使う場合はoffにする)
  "workbench.reduceMotion": "off",
  // パラメータヒントを自動表示（オフの場合はCtrl+Shift+Spaceで表示）
  "editor.parameterHints.enabled": true,
  // ====================================
  // エディタのタイポグラフィーに関する設定
  // ====================================
  // フォント
  "editor.fontFamily": "JetBrains Mono",
  // フォントの合字機能
  "editor.fontLigatures": true,
  // 文字サイズ
  "editor.fontSize": 13.5,
  // 文字間隔
  "editor.letterSpacing": 0,
  // 行間
  "editor.lineHeight": 1.35,
  // エクスプローラーのフォントサイズ
  "window.zoomLevel": 0.8,
  // ====================================
  // キャレットの設定
  // ====================================
  // エディタのキャレットの幅
  "editor.cursorWidth": 3,
  // エディタのキャレットアニメーション
  "editor.cursorBlinking": "phase",
  // ターミナルのキャレットのスタイル
  "terminal.integrated.cursorStyle": "block",
  //ターミナルのフォント
  "terminal.integrated.fontFamily": "monospace",
  // "terminal.integrated.fontFamily": "Hack, 'Noto Sans JP', monospace",
  // ターミナルの文字サイズ
  "terminal.integrated.fontSize": 13.5,
  // ターミナルのキャレットアニメーション
  "terminal.integrated.cursorBlinking": true,
  // テーマカラーの配色を上書きする
  "workbench.colorCustomizations": {
    // エディタのキャレットカラー
    "editorCursor.foreground": "#f00058",
    // ターミナルのキャレットカラー
    "terminalCursor.foreground": "#efff0b",
    "editor.background": "#000000",
    "terminal.background": "#000000",
    // カーソル行の背景色
    // "editor.lineHighlightBackground": "#1a1a1a",
    "editor.lineHighlightBackground": "#000000",
    // "editorLineNumber.activeForeground": "#ff9d00",
    "editor.lineHighlightBorder": "#555555",
    // サイドバーの色
    "sideBar.background": "#0d0d0d",
    "sideBarTitle.foreground": "#a0a0a0",
    "sideBarSectionHeader.background": "#0d0d0d",
    "sideBarSectionHeader.foreground": "#a0a0a0"
    // "[High Contrast Dark]": {
    //   "editor.background": "#1e1e1e",
    //   "editor.foreground": "#d4d4d4",
    //   "activityBarBadge.background": "#007acc",
    //   "sideBarTitle.foreground": "#bbbbbb"
    // }
  },
    // ====================================
  // find it faster
  // ====================================
  "find-it-faster.general.showMaximizedTerminal": false,
  "find-it-faster.findWithinFiles.fuzzRipgrepQuery": true,
  "find-it-faster.general.killTerminalAfterUse": true,
  "multiCommand.commands": [
    {
      "command": "multiCommand.findFiles",
      "interval": 220,
      "sequence": [
        "find-it-faster.findFiles"
      ]
    },
    {
      "command": "multiCommand.findWithinFiles",
      "interval": 220,
      "sequence": [
        "find-it-faster.findWithinFiles"
      ]
    }
  ],
  // ====================================
  // 拡張機能の個別設定
  // ====================================
  // ------------------------------------
  // indent-rainbow
  // ------------------------------------
  // インデントカラー
  "indentRainbow.colors": [
    "rgba(153, 40, 57, 0.24)",
    "rgba(111, 115, 30, 0.24)",
    "rgba(30, 115, 102, 0.24)",
    "rgba(85, 30, 115, 0.24)"
  ],
  // ------------------------------------
  // CodeSnap
  // ------------------------------------
  // 背景色
  "codesnap.backgroundColor": "#70cdc9",
  // エディタの影
  "codesnap.boxShadow": "rgba(0, 0, 0, 0.2) 0px 20px 100px",
  // エディタの行番号表示
  "codesnap.showLineNumbers": false,
  // blameをエディタ内に表示
  "git.blame.editorDecoration.enabled": true,
  // https://help.gitkraken.com/gitlens/gitlens-settings/
  "gitlens.codeLens.codeLens.enabled": false,
  "gitlens.codeLens.currentLine.enabled": false,
  "gitlens.codeLens.hovers.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.toggleLineBlame.enabled": false,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#C77EB5",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "note",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "gitlens.hovers.currentLine.over": "line",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vs-kubernetes": {
    "vs-kubernetes.minikube-show-information-expiration": "2024-07-10T11:05:38.328Z"
  },
  "files.associations": {
    ".skhdrc": "plaintext"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "github.copilot.advanced": {},
  "git.openRepositoryInParentFolders": "never",
  "leetcode.workspaceFolder": "/Users/abe/src/private/leetcode/easy",
  "leetcode.defaultLanguage": "python3",
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.hint.commentDescription": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.venvPath": "/Users/abe/.pyenv",
  "github.copilot.editor.enableAutoCompletions": true,
  "workbench.colorTheme": "Ayu Mirage Zed",
  "github.copilot.nextEditSuggestions.enabled": true,
  "apc.imports": [],
  "vscode_custom_css.imports": [],
  "editor.cursorSurroundingLines": 5,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "custom-ui-style.external.imports": [
    "file://${userHome}/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
  ],
  "animations.Install-Method": "Custom UI Style"
}
